<Window x:Class="Lab02.ListadoSalidas"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Lab02"
        mc:Ignorable="d"
        Title="ListadoSalidas" Height="450" Width="800">
    <Grid>
        <StackPanel>
            <Label Content="Lista de Salidas" FontSize="20" HorizontalAlignment="Center" Margin="0,10,0,10"/>
            <DataGrid ItemsSource="{Binding ListSalidas}" AutoGenerateColumns="True" Name="dgSalidas" Margin="10"/>
        </StackPanel>
    </Grid>
</Window>
